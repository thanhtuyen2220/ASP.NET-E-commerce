
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewData["Title"] - Hubble Space</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="robots" content="all,follow">
    <!-- Bootstrap CSS-->
    <link rel="stylesheet" href="~/distribution/vendor/bootstrap/css/bootstrap.css">
    <!-- Font Awesome CSS-->
    <link rel="stylesheet" href="~/distribution/vendor/font-awesome/css/font-awesome.min.css">
    <!-- Google fonts - Roboto -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700">
    <!-- owl carousel-->
    <link rel="stylesheet" href="~/distribution/vendor/owl.carousel/assets/owl.carousel.css">
    <link rel="stylesheet" href="~/distribution/vendor/owl.carousel/assets/owl.theme.default.css">
    <!-- theme stylesheet-->
    <link rel="stylesheet" href="~/distribution/css/style.default.css" id="theme-stylesheet">
    <!-- Custom stylesheet - for your changes-->
    <link rel="stylesheet" href="~/distribution/css/custom.css">
    <!-- Favicon-->
    <link rel="shortcut icon" href="~/distribution/favicon.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Tweaks for older IEs-->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
    @RenderSection("css", required: false)

</head>
<body>
    <header class="header mb-5">
        <!--
        *** TOPBAR ***
        _________________________________________________________
        -->
        <div id="top">
            <div class="container" style="text-align: end; padding: 0 22pt;">
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="menu list-inline mb-0">
                        <partial name="_LoginInfo" />
                    </ul>
                </div>
            </div>
            <!-- *** TOP BAR END ***-->
        </div>

        <nav class="navbar navbar-expand-lg">
            <div class="container">
                <a href="~/../Home" class="navbar-brand home"><img src="~/Images/Untitled.png" alt="HS logo" class="d-none d-md-inline-block" style="width: 255px"><img src="~/Images/Untitled.png" alt="HS logo" class="d-inline-block d-md-none" style="width: 255px"><span class="sr-only">Hubble Space</span></a>
                <div class="navbar-buttons">
                    <button type="button" data-toggle="collapse" data-target="#navigation" class="btn btn-outline-secondary navbar-toggler"><span class="sr-only">Toggle navigation</span><i class="fa fa-align-justify"></i></button>
                    <button type="button" data-toggle="collapse" data-target="#search" class="btn btn-outline-secondary navbar-toggler"><span class="sr-only">Toggle search</span><i class="fa fa-search"></i></button><a asp-controller="Client_ProductCarts" asp-action="cart" class="btn btn-outline-secondary navbar-toggler"><i class="fa fa-shopping-cart"></i></a>
                </div>
                <div id="navigation" class="collapse navbar-collapse">
                    <ul class="navbar-nav">
                    </ul>
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item"><a asp-controller="Home" asp-action="Index" class="nav-link active">Home</a></li>
                        <li class="nav-item dropdown menu-sidebar">
                            <a asp-action="Categories" asp-route-Object="Men" data-toggle="dropdown" data-hover="dropdown" data-delay="200" class="dropdown-toggle nav-link">Men<b class="caret"></b></a>
                            <ul class="dropdown-menu megamenu">
                                <li>
                                    <div class="row">
                                        <div class="col-md-6 col-lg-4">
                                            <ul class="list-unstyled mb-3">
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Men" asp-route-CategoriesName="Originals" class="nav-link-custom">Originals</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Men" asp-route-CategoriesName="Running" class="nav-link-custom">Running</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Men" asp-route-CategoriesName="Training" class="nav-link-custom">Training</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Men" asp-route-CategoriesName="Football" class="nav-link-custom">Football</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Men" asp-route-CategoriesName="Basketball" class="nav-link-custom">Basketball</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Men" asp-route-CategoriesName="Tennis" class="nav-link-custom">Tennis</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a asp-action="Categories" asp-route-Object="Women" data-toggle="dropdown" data-hover="dropdown" data-delay="200" class="dropdown-toggle nav-link">Women<b class="caret"></b></a>
                            <ul class="dropdown-menu megamenu">
                                <li>
                                    <div class="row">
                                        <div class="col-md-6 col-lg-4">
                                            <ul class="list-unstyled mb-3">
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Women" asp-route-CategoriesName="Originals" class="nav-link-custom">Originals</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Women" asp-route-CategoriesName="Running" class="nav-link-custom">Running</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Women" asp-route-CategoriesName="Training" class="nav-link-custom">Training</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Women" asp-route-CategoriesName="Tennis" class="nav-link-custom">Tennis</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </li>

                        <li class="nav-item dropdown">
                            <a asp-action="Categories" asp-route-Object="Kids" data-toggle="dropdown" data-hover="dropdown" data-delay="200" class="dropdown-toggle nav-link">Kids<b class="caret"></b></a>
                            <ul class="dropdown-menu megamenu">
                                <li>
                                    <div class="row">
                                        <div class="col-md-6 col-lg-4">
                                            <ul class="list-unstyled mb-3">
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Kids" asp-route-CategoriesName="Originals" class="nav-link-custom">Originals</a></li>
                                                <li class="nav-item"><a asp-controller="Home" asp-action="Categories" asp-route-Object="Kids" asp-route-CategoriesName="Running" class="nav-link-custom">Running</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>

                    <div class="navbar-buttons d-flex justify-content-end">
                        <!-- /.nav-collapse-->
                        <div id="search-not-mobile" class="navbar-collapse collapse"></div><a data-toggle="collapse" href="#search" class="btn navbar-btn btn-primary d-none d-lg-inline-block"><span class="sr-only">Toggle search</span><i class="fa fa-search"></i></a>
                        <partial name="_CartInfo" />
                    </div>
                </div>
            </div>
        </nav>
        <div id="search" class="collapse">
            <div class="container">
                <form asp-action="Categories" method="get" class="ml-auto">
                    <div class="input-group">
                        <input name="SearchString" type="text" placeholder="Search" class="form-control" value="@ViewData["Search"]">
                        <div class="input-group-append">
                            <button type="submit" class="btn btn-primary"><i class="fa fa-search"></i></button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </header>
    <div id="all">
        <div id="content">
            @RenderSection("breadcum", required: false)
            @RenderBody()
        </div>
    </div>

    @await Html.PartialAsync("_FrontendFooter")

    <!-- JavaScript files-->
    <script src="~/CoolAdmin-master/vendor/jquery-3.4.1.min.js"></script>
    <script src="~/distribution/vendor/jquery/jquery.min.js"></script>
    <script src="~/distribution/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/distribution/vendor/jquery.cookie/jquery.cookie.js"></script>
    <script src="~/distribution/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/distribution/vendor/owl.carousel2.thumbs/owl.carousel2.thumbs.js"></script>
    <script src="~/distribution/js/front.js"></script>
    @RenderSection("ajax", required: false)
    @RenderSection("Scripts", required: false)

</body>
</html>
